{
    "name": "codecrafting-net/adoldap",
    "description": "A PHP seamless way to search on the Active Directory with ADO and LDAP",
    "keywords": ["ldap", "ado", "adsi", "adodb", "ldap-sso"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Marotta",
            "email": "lucas.marotta@codecrafting.net"
        }
    ],
    "type": "project",
    "require": {
        "php-64bit": ">=7.1",
        "ext-com_dotnet": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "symfony/var-dumper": "^4",
        "devster/ubench": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "CodeCrafting\\AdoLDAP\\": "src/"
        }
    },
    "config": {
        "cache-files-ttl": 0,
        "cache-ttl": 0,
        "cache-files-maxsize": "50GiB"
    }
}
